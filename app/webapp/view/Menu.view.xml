<mvc:View 
    controllerName="treinamento.l4e.app.controller.Menu" 
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:layout= "sap.ui.layout"
    xmlns:tnt="sap.tnt">
    <tnt:ToolPage>
		<tnt:header>
			<tnt:ToolHeader class="Header">
                <ToolbarSpacer width="30%"/> <!-- espaçamento -->
                <Button id="buttonInicio" text="Inicio" press="onPressInicio"/>
                <Button id="buttonLocalidades" text="Localidades" press="onPressLocalidades"/>
                <Button id="buttonVoluntarios" text="Voluntários" press="onPressVoluntarios"/>
                <Button id="buttonDoacoes" text="Doações" press="onPressDoacoes"/>
                <Button id="buttonCalendario" text="Calendário" press="onPressCalendario"/>
                <ToolbarSpacer width="30%"/> <!-- espaçamento -->
			</tnt:ToolHeader>
		</tnt:header>
		<tnt:mainContents>
			<NavContainer id="ConteudoMenu">
			</NavContainer>
		</tnt:mainContents>
    </tnt:ToolPage>
</mvc:View>
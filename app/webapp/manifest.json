{
	"_version": "1.12.0",
	"sap.app": {
		"id": "treinamento.l4e.app",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"ach": "ach",
		"sourceTemplate": {
			"id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
			"version": "1.40.12"
		}
	},
	
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "treinamento.l4e.app.view.App",
			"type": "XML",
			"async": true,
			"id": "App"
		},
		"dependencies": {
			"minUI5Version": "1.60.1",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "treinamento.l4e.app.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "treinamento.l4e.app.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [
                {
                    "name": "RouteApp",
                    "pattern": "RouteApp",
                    "target": ["TargetApp"]
                },
                {
                    "name": "Menu",
                    "pattern": "",
                    "target": ["Menu", "CadastroParceiros"]
                },
                {
                    "name": "CadastroParceiros",
                    "pattern": "CadastroParceiros",
                    "target": ["Menu", "CadastroParceiros"]
                },
                {
                    "name": "ConsultaParceiros",
                    "pattern": "ConsultaParceiros",
                    "target": ["Menu", "ConsultaParceiros"]
                },
                {
                    "name": "EditarParceiros",
                    "pattern": "EditarParceiros/{id}",
                    "target": ["Menu", "CadastroParceiros"]
                }

            ],
			"targets": {
                "Menu": { 
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "menu",
                    "viewName": "Menu",
                    "controlAggregation": "masterPages"
                },
				"TargetApp": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "App",
					"viewName": "App"
                },
                "CadastroParceiros": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "cadastroparceiros",
                    "viewName": "CadastroParceiros",
                    "controlAggregation": "detailPages"
                },
                "ConsultaParceiros": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "consultaparceiros",
                    "viewName": "ConsultaParceiros",
                    "controlAggregation": "detailPages"
                }
			}
		}
	}
}

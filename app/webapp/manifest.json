{
	"_version": "1.12.0",
	"sap.app": {
		"id": "treinamento.l4e.app",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"ach": "ach",
		"sourceTemplate": {
			"id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
			"version": "1.40.12"
		}
	},
	
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "treinamento.l4e.app.view.App",
			"type": "XML",
			"async": true,
			"id": "App"
		},
		"dependencies": {
			"minUI5Version": "1.60.1",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "treinamento.l4e.app.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "treinamento.l4e.app.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [
                {
                    "name": "RouteApp",
                    "pattern": "RouteApp",
                    "target": ["TargetApp"]
                },
                {
                    "name": "Menu",
                    "pattern": "",
                    "target": ["Menu", "Inicio"]
                },
                {
                    "name": "Inicio",
                    "pattern": "",
                    "target": ["Menu", "Inicio"]
                },
                {
                    "name": "CadastroVoluntarios",
                    "pattern": "CadastroVoluntarios",
                    "target": ["Menu", "CadastroVoluntarios"]
                },
                {
                    "name": "ConsultaVoluntarios",
                    "pattern": "ConsultaVoluntarios",
                    "target": ["Menu", "ConsultaVoluntarios"]
                },
                {
                    "name": "EditarVoluntarios",
                    "pattern": "EditarVoluntarios/{id}",
                    "target": ["Menu", "CadastroVoluntarios"]
                },
                {
                    "name": "CadastroDoacoes",
                    "pattern": "CadastroDoacoes",
                    "target": ["Menu", "CadastroDoacoes"]
                },
                {
                    "name": "ConsultaDoacoes",
                    "pattern": "ConsultaDoacoes",
                    "target": ["Menu", "ConsultaDoacoes"]
                },
                {
                    "name": "EditarDoacoes",
                    "pattern": "EditarDoacoes/{id}",
                    "target": ["Menu", "CadastroDoacoes"]
                },
                {
                    "name": "CadastroLocalidade",
                    "pattern": "CadastroLocalidade",
                    "target": ["Menu", "CadastroLocalidade"]
                },
                {
                    "name": "ConsultaLocalidade",
                    "pattern": "ConsultaLocalidade",
                    "target": ["Menu", "ConsultaLocalidade"]
                },
                {
                    "name": "EditarLocalidade",
                    "pattern": "EditarLocalidade/{id}",
                    "target": ["Menu", "CadastroLocalidade"]
                },
                {
                    "name": "CadastroCalendario",
                    "pattern": "CadastroCalendario",
                    "target": ["Menu", "CadastroCalendario"]
                },
                {
                    "name": "ConsultaCalendario",
                    "pattern": "ConsultaCalendario",
                    "target": ["Menu", "ConsultaCalendario"]
                },
                {
                    "name": "EditarCalendario",
                    "pattern": "EditarCalendario/{id}",
                    "target": ["Menu", "CadastroCalendario"]
                }

            ],
			"targets": {
                "Menu": { 
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "menu",
                    "viewName": "Menu"
                },
				"TargetApp": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "App",
					"viewName": "App"
                },
                "Inicio": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "inicio",
                    "viewName": "Inicio",
                    "controlAggregation": "pages",
                    "parent": "Menu",
                    "controlId": "ConteudoMenu"
                },
                "CadastroVoluntarios": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "cadastroVoluntarios",
                    "viewName": "CadastroVoluntarios",
                    "controlAggregation": "pages",
                    "parent": "Menu",
                    "controlId": "ConteudoMenu"
                },
                "ConsultaVoluntarios": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "consultaVoluntarios",
                    "viewName": "ConsultaVoluntarios",
                    "controlAggregation": "pages",
                    "parent": "Menu",
                    "controlId": "ConteudoMenu"
                },
                "CadastroDoacoes": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "cadastroDoacoes",
                    "viewName": "CadastroDoacoes",
                    "controlAggregation": "pages",
                    "parent": "Menu",
                    "controlId": "ConteudoMenu"
                },
                "ConsultaDoacoes": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "consultaDoacoes",
                    "viewName": "ConsultaDoacoes",
                    "controlAggregation": "pages",
                    "parent": "Menu",
                    "controlId": "ConteudoMenu"
                },
                "CadastroLocalidade": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "cadastroLocalidade",
                    "viewName": "CadastroLocalidade",
                    "controlAggregation": "pages",
                    "parent": "Menu",
                    "controlId": "ConteudoMenu"
                },
                "ConsultaLocalidade": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "consultaLocalidade",
                    "viewName": "ConsultaLocalidade",
                    "controlAggregation": "pages",
                    "parent": "Menu",
                    "controlId": "ConteudoMenu"
                },
                "CadastroCalendario": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "cadastroCalendario",
                    "viewName": "CadastroCalendario",
                    "controlAggregation": "pages",
                    "parent": "Menu",
                    "controlId": "ConteudoMenu"
                },
                "ConsultaCalendario": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "consultaCalendario",
                    "viewName": "ConsultaCalendario",
                    "controlAggregation": "pages",
                    "parent": "Menu",
                    "controlId": "ConteudoMenu"
                }
			}
		}
	}
}
